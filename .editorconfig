# EditorConfig: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
spelling_language = en-US

# Unix-style newlines
end_of_line = lf

insert_final_newline = true
trim_trailing_whitespace = true

[*.{c,h}]
indent_style = space
indent_size = 2
max_line_length = 79
